{
    "name": "Code_Reactor",
    "homepage": "http://mourtz.github.io/CodeReactor",
    "description": "Hackable to the core homegrown code editor.",
    "bugs": "https://github.com/Mourtz/CodeReactor/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/Mourtz/CodeReactor.git"
    },
    "license": "MIT",
    "main": "index.html",
    "version": "0.0.1",
    "window": {
        "title": "Code Reactor",
        "icon": "icon.png",
        "width": 960,
        "height": 520,
        "min_width": 960,
        "min_height": 520,
        "resizable": true,
        "toolbar": true,
        "frame": false,
        "position": "center"
    },
    "scripts": {
        "precommit": "npm run beautify",
        "beautify": "node scripts/Beautify.js",
        "clean": "node scripts/Clean.js",
        "minify": "node scripts/Minify.js",
        "build": "npm run minify && grunt && npm run clean",
        "start": "nw ."
    },
    "dependencies": {
        "app-root-path": "^1.2.0",
        "bootstrap": "^3.3.5",
        "codemirror": "^5.14.2",
        "colors": "^1.1.2",
        "grunt": "^0.4.5",
        "isbinaryfile": "^3.0.0",
        "jquery": "^2.2.3",
        "jquery.hotkeys": "github:jeresig/jquery.hotkeys",
        "js-beautify": "^1.5.10",
        "jslint": "^0.9.5",
        "jsonfile": "^2.3.0",
        "react": "^0.14.8",
        "requirejs": "^2.2.0",
        "shelljs": "^0.6.0",
        "uglify-js": "^2.6.2",
        "uglifycss": "0.0.19"
    },
    "devDependencies": {
        "grunt": "^0.4.5",
        "grunt-contrib-copy": "^0.8.2",
        "grunt-nw-builder": "^2.0.0",
        "husky": "^0.10.2",
        "nw-builder": "^2.2.0"
    }
}
